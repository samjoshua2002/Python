a=int(input('Enter the number to find the tables: '))
print(a,"* 1 =", a*1)
print(a,"* 2 =", a*2)
print(a,"* 3 =", a*3)
print(a,"* 4 =", a*4)
print(a,"* 5 =", a*5)
print(a,"* 6 =", a*6)
print(a,"* 7 =", a*7)
print(a,"* 8 =", a*8)
print(a,"* 9 =", a*9)
print(a,"* 10 =", a*10)
print(a,"* 11 =", a*11)
print(a,"* 12 =", a*12)
